# Box JWT Configuration
# Get these values from your Box Custom App in the Box Developer Console
# Download the JSON config file and copy these values

BOX_CLIENT_ID=your_client_id_here
BOX_CLIENT_SECRET=your_client_secret_here
BOX_ENTERPRISE_ID=your_enterprise_id_here
BOX_PUBLIC_KEY_ID=your_public_key_id_here
BOX_PRIVATE_KEY="-----BEGIN ENCRYPTED PRIVATE KEY-----\nyour_private_key_here\n-----END ENCRYPTED PRIVATE KEY-----"
BOX_PASSPHRASE=your_passphrase_here

# Box Folder IDs
# After creating your root folder structure in Box, add the folder IDs here
BOX_ROOT_FOLDER_ID=0
BOX_PROJECTS_FOLDER_ID=0

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Gemini AI Configuration
# Get your API key from Google AI Studio: https://aistudio.google.com/
# Recommended models: gemini-1.5-pro for complex tasks, gemini-1.5-flash for faster, cheaper tasks.
# Ensure your project has access to these models.
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL_ID=gemini-1.5-pro

# Box Webhook Configuration
# This secret is used to verify the authenticity of incoming Box webhook events.
# Generate a strong, random string (e.g., using a password generator).
BOX_WEBHOOK_SECRET=your_box_webhook_secret_here
